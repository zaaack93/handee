    "8fcd9fe8-9b37-4851-94af-c116aff7900e": {
      "type": "image-with-text",
      "settings": {
        "display_on_card": false,
        "image": "shopify:\/\/shop_images\/SORB_MAY_LS_WEB-51.jpg",
        "crop": "aspect-w-1 aspect-h-1",
